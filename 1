[11:11 pm, 24/06/2024] Shah Neil Khan: // main.dart
class Media {
  void play() {
    print('Playing media...');
  }
}

class Song extends Media {
  String artist;

  Song(this.artist);

  @override
  void play() {
    print('Playing song by $artist...');
  }
}

void main() {
  Media media = Media();
  media.play();

  Song song = Song('Adele');
  song.play();
}
